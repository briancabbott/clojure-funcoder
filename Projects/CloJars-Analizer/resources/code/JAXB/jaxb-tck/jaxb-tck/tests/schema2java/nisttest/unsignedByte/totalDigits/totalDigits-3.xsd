<?xml version="1.0"?>

<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:nist="NISTSchema-unsignedByte-totalDigits-3-NS"
        targetNamespace="NISTSchema-unsignedByte-totalDigits-3-NS">

  <annotation>
    <documentation>
      The National Institute of Standards and Technology.
      Conformance test for W3C XML Schema Part 2: Datatypes.
      (See http://www.w3.org/TR/2001/PR-xmlschema-2-20010330/)
      Created Sep 24, 2001.

      File: totalDigits-3.xsd
      This schema specifies a datatype, NISTSchema-unsignedByte-totalDigits-3,
      which is derived by restriction of built-in type unsignedByte
      by facet totalDigits with a value of: 3
    </documentation>
  </annotation>

  <element name="NISTSchema-unsignedByte-totalDigits-3" type="nist:NISTSchema-unsignedByte-totalDigits-3-Type"/>

  <simpleType name="NISTSchema-unsignedByte-totalDigits-3-Type">
    <restriction base="unsignedByte">
      <totalDigits value="3"/>
    </restriction>
  </simpleType>

</schema>
